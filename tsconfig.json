{
	"compilerOptions": {
		"target": "es2018",
		"outDir": "dist",
		"sourceMap": true,
		"newLine": "LF",
		"allowJs": true,
		"module": "commonjs",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"stripInternal": true,
		"downlevelIteration": true,
		"preserveConstEnums": true,
		"experimentalDecorators": true,
		"jsx": "react"
	}
}