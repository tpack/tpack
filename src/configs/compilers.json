[
	{
		"match": [
			"*.{js,jsx}",
			"!**/node_modules/**"
		],
		"use": "tpack/compilers/typescript"
	},
	{
		"match": [
			"*.{ts,tsx}",
			"!*.d.ts"
		],
		"use": "tpack/compilers/typescript"
	},
	{
		"match": [
			"*.coffee"
		],
		"use": "tpack/compilers/coffeescript"
	},
	{
		"match": "*.less",
		"use": "tpack/compilers/less"
	},
	{
		"match": "*.{sass,scss}",
		"use": "tpack/compilers/sass"
	},
	{
		"match": "*.styl",
		"use": "tpack/compilers/stylus"
	}
]